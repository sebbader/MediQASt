<?xml version="1.0"?>
<!-- patterns which comply with the condition are converted to the triple 
	format "(first second third)" -->
<!-- GrammaticalRelation format is "<GrammaticalRelation>(<governor>, <dependant>)" -->
<rules>
	<!-- connected rules -->
	<rule priority="normal">
		<condition>
			<GrammaticalRelation>nsubj</GrammaticalRelation>
			<gov>first</gov>
			<dep>second</dep>
		</condition>
		<condition>
			<GrammaticalRelation>nmod:of</GrammaticalRelation>
			<gov>second</gov>
			<dep>third</dep>
		</condition>
		<condition>
			<GrammaticalRelation>nmod:of</GrammaticalRelation>
			<gov>third</gov>
			<dep>fourth</dep>
		</condition>
	</rule>
	<rule priority="normal">
		<condition>
			<GrammaticalRelation>nsubj</GrammaticalRelation>
			<gov>fourth</gov>
			<dep>first</dep>
		</condition>
		<condition>
			<GrammaticalRelation>dobj</GrammaticalRelation>
			<gov>fourth</gov>
			<dep>second</dep>
		</condition>
		<condition>
			<GrammaticalRelation>nmod:with</GrammaticalRelation>
			<gov>second</gov>
			<dep>third</dep>
		</condition>
	</rule>
	
	<!-- standard rules -->
	<rule priority="normal">
		<condition>
			<GrammaticalRelation>nsubj</GrammaticalRelation>
			<gov>second</gov>
			<dep>first</dep>
		</condition>
		<condition>
			<GrammaticalRelation>dobj</GrammaticalRelation>
			<gov>second</gov>
			<dep>third</dep>
		</condition>
	</rule>
	<rule priority="normal">
		<condition>
			<GrammaticalRelation>nsubj</GrammaticalRelation>
			<gov>first</gov>
			<dep>second</dep>
		</condition>
		<condition>
			<GrammaticalRelation>nmod:of</GrammaticalRelation>
			<gov>second</gov>
			<dep>third</dep>
		</condition>
	</rule>
	<rule priority="normal">
		<condition>
			<GrammaticalRelation>nsubj</GrammaticalRelation>
			<gov>second</gov>
			<dep>first</dep>
		</condition>
		<condition>
			<GrammaticalRelation>nmod:against</GrammaticalRelation>
			<gov>second</gov>
			<dep>third</dep>
		</condition>
	</rule>
	<rule priority="normal">
		<condition>
			<GrammaticalRelation>nsubjpass</GrammaticalRelation>
			<gov>second</gov>
			<dep>first</dep>
		</condition>
		<condition>
			<GrammaticalRelation>nmod:agent</GrammaticalRelation>
			<gov>second</gov>
			<dep>third</dep>
		</condition>
	</rule>
	<rule priority="normal">
		<condition>
			<GrammaticalRelation>nsubjpass</GrammaticalRelation>
			<gov>second</gov>
			<dep>first</dep>
		</condition>
		<condition>
			<GrammaticalRelation>nmod:for</GrammaticalRelation>
			<gov>second</gov>
			<dep>third</dep>
		</condition>
	</rule>
	
	<!-- regex rules -->
	<rule priority="normal">
		<condition>
			<GrammaticalRelation>det</GrammaticalRelation>
			<gov>"which"</gov>
			<dep>third</dep>
		</condition>
		<implication>
			<first>"which"</first>
			<second>"type"</second>
			<third>third</third>
		</implication>
	</rule>
	<rule priority="normal">
		<condition>
			<GrammaticalRelation>det</GrammaticalRelation>
			<gov>third</gov>
			<dep>"which"</dep>
		</condition>
		<implication>
			<first>"which"</first>
			<second>"type"</second>
			<third>third</third>
		</implication>
	</rule>
	<rule priority="normal">
		<condition>
			<GrammaticalRelation>det</GrammaticalRelation>
			<gov>third</gov>
			<dep>"what"</dep>
		</condition>
		<implication>
			<first>"what"</first>
			<second>"type"</second>
			<third>third</third>
		</implication>
	</rule>
	<rule priority="normal">
		<condition>
			<GrammaticalRelation>det</GrammaticalRelation>
			<gov>"what"</gov>
			<dep>third</dep>
		</condition>
		<implication>
			<first>"what"</first>
			<second>"type"</second>
			<third>third</third>
		</implication>
	</rule>
	<rule priority="normal">
		<condition>
			<GrammaticalRelation>det</GrammaticalRelation>
			<gov>third</gov>
			<dep>"who"</dep>
		</condition>
		<implication>
			<first>"who"</first>
			<second>"type"</second>
			<third>"person"</third>
		</implication>
	</rule>
	<rule priority="normal">
		<condition>
			<GrammaticalRelation>det</GrammaticalRelation>
			<gov>"who"</gov>
			<dep>third</dep>
		</condition>
		<implication>
			<first>"who"</first>
			<second>"type"</second>
			<third>"person"</third>
		</implication>
	</rule>
	
	<!-- default rule for keyword query -->
	<rule priority="default">
		<condition>
			<GrammaticalRelation>nsubj</GrammaticalRelation>
			<gov>"wh"</gov>
			<dep>third</dep>
		</condition>
		<implication>
			<first>"VARIABLE"</first>
			<second>"sameAs"</second>
			<third>third</third>
		</implication>
	</rule>
	
	<CombinedGrammaticalRelationsToMerge>
		<mergeRule>
			<condition>
				<GrammaticalRelation deleteAfterCompress="false">
					dobj
				</GrammaticalRelation>
				<gov>first</gov>
				<dep>x</dep>
			</condition>
			<condition>
				<GrammaticalRelation deleteAfterCompress="true">
					compound
				</GrammaticalRelation>
				<gov>x</gov>
				<dep>second</dep>
			</condition>
		</mergeRule>
	</CombinedGrammaticalRelationsToMerge>
	<GrammaticalRelationsToMerge>
		<GrammaticalRelation deleteAfterCompress="false">det</GrammaticalRelation>
		<GrammaticalRelation deleteAfterCompress="true">case</GrammaticalRelation>
		<GrammaticalRelation deleteAfterCompress="true">nmod:poss</GrammaticalRelation>
		<GrammaticalRelation deleteAfterCompress="true">amod</GrammaticalRelation>
		<GrammaticalRelation deleteAfterCompress="true">cc</GrammaticalRelation>
		<GrammaticalRelation deleteAfterCompress="true">conj:or</GrammaticalRelation>
		<GrammaticalRelation deleteAfterCompress="true">cop</GrammaticalRelation>
		<GrammaticalRelation deleteAfterCompress="true">advmod</GrammaticalRelation>
		<GrammaticalRelation deleteAfterCompress="true">aux</GrammaticalRelation>
		<GrammaticalRelation deleteAfterCompress="true">auxpass</GrammaticalRelation>
		<GrammaticalRelation deleteAfterCompress="true">conj:and</GrammaticalRelation>
		<GrammaticalRelation deleteAfterCompress="true">amod</GrammaticalRelation>
		<GrammaticalRelation deleteAfterCompress="true">dep</GrammaticalRelation>
	</GrammaticalRelationsToMerge>
</rules>